{
  "engines": {
    "node": ">=24.0.0"
  },
  "workspaces": [
    "frontend/*",
    "oldplunger"
  ],
  "scripts": {
    "build:oldplunger": "npm run build --workspace=oldplunger",
    "build:selector": "npm run build --workspace=frontend/selector",
    "build:admin": "npm run build --workspace=frontend/admin",
    "build:all": "npm run build:selector && npm run build:admin && npm run build:oldplunger",
    "start:server": "node server/index.ts",
    "start": "npm run build:all && npm run start:server",
    "dev:admin": "npm run dev --workspace=frontend/admin",
    "dev:selector": "npm run dev --workspace=frontend/selector",
    "dev:oldplunger": "npm run dev --workspace=oldplunger",
    "prettier": "npx prettier . --write",
    "lint": "npm run prettier && npx eslint .",
    "lint:fix": "npm run prettier && npx eslint --fix ."
  },
  "dependencies": {
    "@distube/ytdl-core": "^4.16.12",
    "@sapphire/snowflake": "^3.5.5",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "bcrypt": "^6.0.0",
    "cheerio": "^1.1.2",
    "cookie-parser": "^1.4.7",
    "cross-env": "^10.1.0",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "fluent-ffmpeg": "^2.1.3",
    "jimp": "^1.6.0",
    "libsodium-wrappers": "^0.8.0",
    "mediasoup": "^3.19.14",
    "multer": "^2.0.2",
    "murmurhash-js": "^1.0.0",
    "pg": "^8.16.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.11.0",
    "react-transition-group": "^4.4.5",
    "semantic-sdp": "^3.31.1",
    "speakeasy": "^2.0.0",
    "vite": "npm:rolldown-vite@7.3.0",
    "vite-plugin-svgr": "^4.5.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@eslint/css": "^0.14.1",
    "@eslint/js": "^9.39.2",
    "@eslint/json": "^0.14.0",
    "@eslint/markdown": "^7.5.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^17.0.0",
    "jiti": "^2.6.1",
    "prettier": "3.7.4",
    "typescript-eslint": "^8.52.0"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.3.0"
  },
  "optionalDependencies": {
    "@spacebarchat/erlpack": "^1.0.1"
  }
}
