{
  "engines": {
    "node": ">=24.0.0"
  },
  "workspaces": [
    "frontend/*",
    "oldplunger"
  ],
  "scripts": {
    "build:server": "tsc -p server",
    "build:oldplunger": "npm run build --workspace=oldplunger",
    "build:selector": "npm run build --workspace=frontend/selector",
    "build:admin": "npm run build --workspace=frontend/admin",
    "build:all": "npm run build:selector && npm run build:admin && npm run build:oldplunger && npm run build:server",
    "start:server": "node server/dist/index.js",
    "start": "npm run build:all && npm run start:server",
    "dev:admin": "npm run dev --workspace=frontend/admin",
    "dev:selector": "npm run dev --workspace=frontend/selector",
    "dev:server": "nodemon server/src/index.ts",
    "dev:oldplunger": "npm run dev --workspace=oldplunger"
  },
  "dependencies": {
    "@distube/ytdl-core": "^4.16.12",
    "@sapphire/snowflake": "^3.5.5",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "bcrypt": "^6.0.0",
    "cheerio": "^1.1.2",
    "cookie-parser": "^1.4.7",
    "cross-env": "^10.1.0",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "fluent-ffmpeg": "^2.1.3",
    "jimp": "^1.6.0",
    "libsodium-wrappers": "^0.8.0",
    "mediasoup": "^3.19.14",
    "multer": "^2.0.2",
    "murmurhash-js": "^1.0.0",
    "pg": "^8.16.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.11.0",
    "react-transition-group": "^4.4.5",
    "semantic-sdp": "^3.31.1",
    "speakeasy": "^2.0.0",
    "vite": "npm:rolldown-vite@7.3.0",
    "vite-plugin-svgr": "^4.5.0",
    "ws": "^8.19.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.6",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/murmurhash-js": "^1.0.6",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.18.1",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.3.0"
  },
  "optionalDependencies": {
    "@spacebarchat/erlpack": "^1.0.1"
  }
}
