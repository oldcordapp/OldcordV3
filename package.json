{
  "engines": {
    "node": ">=24.0.0"
  },
  "workspaces": [
    "frontend/*",
    "oldplunger"
  ],
  "scripts": {
    "build:oldplunger": "npm run build --workspace=oldplunger",
    "build:selector": "npm run build --workspace=frontend/selector",
    "build:admin": "npm run build --workspace=frontend/admin",
    "build:all": "npm run build:selector && npm run build:admin && npm run build:oldplunger",
    "start:server": "node server/index.js",
    "start": "npm run build:all && npm run start:server",
    "dev:admin": "npm run dev --workspace=frontend/admin",
    "dev:selector": "npm run dev --workspace=frontend/selector",
    "dev:oldplunger": "npm run dev --workspace=oldplunger"
  },
  "dependencies": {
    "@distube/ytdl-core": "^4.16.12",
    "@google-cloud/storage": "^7.18.0",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "base64url": "^3.0.1",
    "bcrypt": "^6.0.0",
    "cheerio": "^1.1.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cross-env": "^10.1.0",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "jimp": "^1.6.0",
    "libsodium-wrappers": "^0.7.15",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "mediasoup": "^3.19.14",
    "multer": "^2.0.2",
    "murmurhash-js": "^1.0.0",
    "pg": "^8.16.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.11.0",
    "react-transition-group": "^4.4.5",
    "semantic-sdp": "^3.31.1",
    "speakeasy": "^2.0.0",
